{
    "go_no_golangci_lint": {
        "image": "archlinux:latest",
        "features": {
            "go": {
                "golangciLintVersion": "none"
            }
        }
    },
    "install_air": {
        "image": "archlinux:latest",
        "features": {
            "go": {
                "installAir": true
            }
        }
    },
    "install_cobra_cli": {
        "image": "archlinux:latest",
        "features": {
            "go": {
                "installCobraCli": true
            }
        }
    },
    "install_go_releaser": {
        "image": "archlinux:latest",
        "features": {
            "go": {
                "installGoReleaser": true
            }
        }
    },
    "install_gox": {
        "image": "archlinux:latest",
        "features": {
            "go": {
                "installGox": true
            }
        }
    },
    "install_ko": {
        "image": "archlinux:latest",
        "features": {
            "go": {
                "installKo": true
            }
        }
    },
    "install_yaegi": {
        "image": "archlinux:latest",
        "features": {
            "go": {
                "installYaegi": true
            }
        }
    }
}